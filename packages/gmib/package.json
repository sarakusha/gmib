{
  "name": "@nibus/gmib",
  "version": "1.6.5",
  "description": "Nata-Info gmib tool",
  "keywords": [
    "nata-info",
    "nibus",
    "sarakusha"
  ],
  "license": "MIT",
  "author": "Andrei Sarakeev <avs@nata-info.ru>",
  "publishConfig": {
    "registry": "https://npm.nata-info.ru"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarakusha/nibus.git"
  },
  "bugs": {
    "url": "https://github.com/sarakusha/nibus/issues"
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "cross-env NODE_ENV=production electron-webpack",
    "dist": "electron-builder",
    "dist:mac": "yarn dist -c.mac.identity=null",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "dist:linux": "yarn dist -l",
    "dist:win": "yarn dist -w --x64",
    "publish": "yarn compile && yarn release",
    "release": "env-cmd -f ./.env electron-builder -lwm --x64 -c.mac.identity=null",
    "type": "tsc --project tsconfig.json --noEmit",
    "showMainConfig": "node src/util/show-config.js main",
    "showRendererConfig": "node src/util/show-config.js renderer",
    "analyze": "cross-env ANALYZE=1 electron-webpack dev",
    "lint:dev": "eslint --ext .js,.ts,.tsx src/",
    "lint": "eslint -c ../../.eslintrc-prod.js --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint -c ../../.eslintrc-prod.js --ext .js,.ts,.tsx --fix src/"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@nibus/core": "^1.6",
    "classnames": "^2.2.6",
    "electron": "^7.1.9",
    "electron-json-storage": "^4.1.8",
    "electron-log": "^4.0.3",
    "electron-updater": "^4.2.0",
    "hotkeys-js": "^3.7.3",
    "immer": "^5.3.2",
    "lerna": "^3.20.2",
    "lodash": "^4.17.15",
    "rc-slider": "^9.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.0",
    "react-input-mask": "^2.0.4",
    "react-swipeable-views": "^0.13.1",
    "recompose": "^0.30.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.11",
    "typeface-roboto": "^0.0.75",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-json-storage": "^4.0.0",
    "@types/material-ui": "^0.21.6",
    "@types/rc-slider": "^8.6.3",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-input-mask": "^2.0.1",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/recompose": "^0.30.5",
    "@types/semver": "^6.0.0",
    "@types/warning": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "css-loader": "^3.0.0",
    "electron-builder": "^22.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.1.1",
    "env-cmd": "^10.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^4.0.0",
    "tslint-react-a11y": "^1.0.0",
    "tslint-react-hooks": "^2.1.1",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-build-notifier": "^2.0.0",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "build": {
    "appId": "ru.nata-info.gmib",
    "copyright": "Copyright Â© Nata-Info, 2019",
    "productName": "gmib",
    "publish": "github",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "category": "Development",
      "target": "AppImage",
      "executableName": "gmib"
    },
    "nsis": {
      "artifactName": "${productName}-setup-${version}.${ext}"
    },
    "appImage": {
      "artifactName": "${productName}-${version}.${ext}"
    },
    "directories": {
      "buildResources": "resources"
    },
    "extraResources": [
      {
        "from": "./src/extraResources/",
        "to": "extraResources",
        "filter": [
          "**/*"
        ]
      }
    ],
    "electronVersion": "6.0.7"
  },
  "gitHead": "522054682a29e66b40cfa80e765780ff654be82b"
}
