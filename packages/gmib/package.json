{
  "name": "gmib",
  "version": "3.5.4",
  "description": "Nata-Info gmib tool",
  "private": true,
  "keywords": [
    "nata-info",
    "nibus",
    "sarakusha"
  ],
  "license": "MIT",
  "author": "Andrei Sarakeev <avs@nata-info.ru>",
  "repository": {
    "type": "git",
    "url": "https://github.com/sarakusha/nibus.git",
    "directory": "packages/gmib"
  },
  "bugs": {
    "url": "https://github.com/sarakusha/nibus/issues"
  },
  "scripts": {
    "dev": "electron-webpack dev --no-sandbox",
    "dev:mock": "cross-env MOCKED_NIBUS=1 electron-webpack dev",
    "compile": "cross-env NODE_ENV=production ELECTRON_ENABLE_LOGGING=1 electron-webpack",
    "dist": "electron-builder",
    "dist:mac": "yarn dist",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "dist:linux": "yarn dist -l",
    "dist:win": "yarn dist -w --x64 --ia32",
    "dist:win:dir": "yarn dist -w --x64 --dir",
    "_publish": "yarn compile && yarn release",
    "release:mac": "electron-builder -p always -m --x64",
    "release:linux": "electron-builder -p always -l --x64",
    "release:win": "electron-builder -p always -w --x64 --ia32",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "showMainConfig": "node show-config.js main > maincfg",
    "showRendererConfig": "node show-config.js renderer > ./rendercfg",
    "analyze": "cross-env ANALYZE=1 electron-webpack dev",
    "lint:dev": "eslint --ext .js,.ts,.tsx src/",
    "lint": "eslint -c ../../.eslintrc-prod.js --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint -c ../../.eslintrc-prod.js --ext .js,.ts,.tsx --fix src/",
    "postinstall": "electron-builder install-app-deps",
    "yalc:novastar": "yalc add @novastar/codec @novastar/net @novastar/screen @novastar/native",
    "download-ffmpeg-mac": "mkdirp ffmpeg/darwin-x64 && cd ffmpeg/darwin-x64 && wget https://github.com/mifi/ffmpeg-build-script/releases/download/4.4.1-2/ffmpeg -O ffmpeg && wget https://github.com/mifi/ffmpeg-build-script/releases/download/4.4.1-2/ffprobe -O ffprobe && chmod +x ffmpeg && chmod +x ffprobe",
    "download-ffmpeg-linux": "mkdirp ffmpeg/linux-x64 && cd ffmpeg/linux-x64 && wget https://github.com/mifi/ffmpeg-builds/releases/download/4.4.1/ffmpeg-release-amd64-static.tar.xz -O ffmpeg.xz && tar xvf ffmpeg.xz ffmpeg-4.4.1-amd64-static/ffmpeg ffmpeg-4.4.1-amd64-static/ffprobe --strip-components=1",
    "download-ffmpeg-windows": "mkdirp ffmpeg/win32-x64 && cd ffmpeg/win32-x64 && npx download-cli https://github.com/mifi/ffmpeg-builds/releases/download/4.4.1/ffmpeg-n4.4.1-2-gcc33e73618-win64-gpl-4.4.zip --out . --filename ffmpeg.zip && 7z x ffmpeg.zip && npx shx mv ffmpeg-n4.4.1-2-gcc33e73618-win64-gpl-4.4/bin/ffmpeg.exe ./ && npx shx mv ffmpeg-n4.4.1-2-gcc33e73618-win64-gpl-4.4/bin/ffprobe.exe ./"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@mdx-js/react": "^1.6.22",
    "@nibus/cli": "^3.5.4",
    "@nibus/core": "^3.5.4",
    "@novastar/codec": "file:.yalc/@novastar/codec",
    "@novastar/native": "file:.yalc/@novastar/native",
    "@novastar/net": "file:.yalc/@novastar/net",
    "@novastar/screen": "file:.yalc/@novastar/screen",
    "@novastar/serial": "file:.yalc/@novastar/serial",
    "@reduxjs/toolkit": "^1.5.0",
    "ajv": "^8.0.5",
    "ansicolor": "^1.1.93",
    "bonjour-hap": "^3.6.2",
    "classnames": "^2.2.6",
    "color-hash": "^2.0.1",
    "debug": "^4.3.1",
    "dotenv": "^11.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.0.3",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.2.0",
    "execa": "^6.1.0",
    "express": "^4.17.3",
    "file-type": "^17.1.1",
    "formidable": "v2",
    "helmet": "^5.0.2",
    "highcharts": "^9.0.0",
    "highcharts-react-official": "^3.0.0",
    "hotkeys-js": "^3.7.3",
    "iput": "^1.1.2",
    "lodash": "^4.17.15",
    "material-ui-chip-input": "^2.0.0-beta.2",
    "node-machine-id": "^1.1.12",
    "notistack": "^1.0.1",
    "range-parser": "^1.2.1",
    "rc-slider": "^9.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-imask": "6.1",
    "react-redux": "^7.2.2",
    "sanitize-html": "^2.3.2",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.20",
    "sqlite3": "^5.0.2",
    "suncalc": "^1.8.0",
    "supports-color": "^8",
    "tail": "^2.2.0",
    "tcp-port-used": "^1.0.2",
    "tiny-typed-emitter": "^2.0.3",
    "typeface-roboto": "^1.1.13",
    "usb-detection": "https://github.com/sarakusha/node-usb-detection.git#fix_segfault_on_close",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@mdx-js/loader": "^1.6.22",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@types/classnames": "^2.2.7",
    "@types/color-hash": "^1.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-json-storage": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.4",
    "@types/material-ui": "^0.21.6",
    "@types/mdx-js__react": "^1.5.5",
    "@types/range-parser": "^1.2.3",
    "@types/rc-slider": "^9.3.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-input-mask": "^3.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/sanitize-html": "^2.3.1",
    "@types/sqlite3": "^3.1.7",
    "@types/suncalc": "^1.8.0",
    "@types/tcp-port-used": "^1.0.0",
    "@types/valid-url": "^1.0.3",
    "@types/webpack-env": "^1.16.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.1",
    "css-loader": "5",
    "cz-conventional-changelog": "3.3.0",
    "devtron": "^1.4.0",
    "electron": "17.2.0",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.1.1",
    "electron-react-devtools": "^0.5.3",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^4.0.1",
    "env-cmd": "^10.0.1",
    "react-refresh": "^0.11.0",
    "webpack": "4",
    "webpack-build-notifier": "^2.0.0"
  },
  "build": {
    "npmRebuild": true,
    "appId": "ru.nata-info.gmib",
    "copyright": "Copyright Â© Nata-Info, 2021",
    "productName": "gmib",
    "publish": "github",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dmg",
        "zip"
      ],
      "extraResources": [
        {
          "from": "ffmpeg/darwin-${arch}/ffmpeg",
          "to": "ffmpeg"
        },
        {
          "from": "ffmpeg/darwin-${arch}/ffprobe",
          "to": "ffprobe"
        }
      ]
    },
    "linux": {
      "category": "Utility",
      "target": "AppImage",
      "desktop": {
        "Name": "${productName}",
        "Terminal": "false"
      }
    },
    "nsis": {
      "artifactName": "${productName}-setup-${version}.${ext}",
      "extraResources": [
        {
          "from": "ffmpeg/win32-${arch}/ffmpeg.exe",
          "to": "ffmpeg.exe"
        },
        {
          "from": "ffmpeg/win32-${arch}/ffprobe.exe",
          "to": "ffprobe.exe"
        }
      ]
    },
    "appImage": {
      "artifactName": "${productName}.${ext}",
      "extraResources": [
        {
          "from": "ffmpeg/linux-${arch}/ffmpeg",
          "to": "ffmpeg"
        },
        {
          "from": "ffmpeg/linux-${arch}/ffprobe",
          "to": "ffprobe"
        }
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "extraResources": [
      {
        "from": "./src/extraResources/",
        "to": "extraResources",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "debug": "^4.3.1"
  }
}
