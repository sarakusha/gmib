{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-react-a11y",
    "tslint-react-hooks"
  ],
  "jsRules": {},
  "rules": {
    "no-unused-variable": false,
    "import-name": false,
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "react-hooks-nesting": "error"
  },
  "rulesDirectory": []
}
