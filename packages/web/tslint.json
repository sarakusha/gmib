{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "import-name": false,
        "jsx-boolean-value": false,
        "jsx-no-multiline-js": false,
        "semicolon": [true, "always", "ignore-bound-class-methods"]
    },
    "rulesDirectory": []
}
