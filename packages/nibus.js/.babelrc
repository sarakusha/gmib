{
  "env": {
    "development": {
      "sourceMaps": "inline",
      "plugins": [
        "source-map-support"
      ]
    }
  },
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    [
      "@babel/preset-stage-0",
      {
        "decoratorsLegacy": true
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import"
  ]
}
