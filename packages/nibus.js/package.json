{
  "name": "@nata/nibus.js",
  "version": "1.3.6",
  "description": "Nata-Info NiBUS",
  "keywords": [
    "nata-info",
    "nibus",
    "sarakusha"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "service": "cross-env NODE_ENV=development DEBUG=nibus babel-node --extensions \".ts,.tsx,.js\" src/index.ts start",
    "build": "cross-env BABEL_ENV=production yarn clean && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts,.tsx,.js\" && chmod +x lib/index.js",
    "clean": "rimraf -rf lib tsconfig.tsbuildinfo"
  },
  "bin": {
    "nibus": "lib/index.js"
  },
  "author": "Andrei Sarakeev <avs@nata-info.ru>",
  "license": "SEE LICENSE IN EULA.md",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "EULA.md",
    "detection.yml"
  ],
  "publishConfig": {
    "registry": "https://npm.nata-info.ru"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarakusha/nata.git"
  },
  "bugs": {
    "url": "https://github.com/sarakusha/nata/issues"
  },
  "private": false,
