{
  "name": "gmib",
  "version": "3.6.0",
  "description": "Nata-Info gmib tool",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "keywords": [
    "nata-info",
    "nibus",
    "sarakusha"
  ],
  "license": "MIT",
  "author": "Andrei Sarakeev <avs@nata-info.ru>",
  "main": "packages/main/dist/index.cjs",
  "repository": "https://github.com/sarakusha/gmib.git",
  "scripts": {
    "build": "cross-env MODE=production run-p build:main build:preload build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:preload:types": "dts-cb -i packages/preload/tsconfig.json -o packages/preload/exposedInMainWorld.d.ts",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "electron-builder build --config .electron-builder.config.js --dir",
    "test": "run-s test:*",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "test:e2e": "npm run build && vitest run",
    "watch": "rimraf packages/main/dist/*.js && node --trace-warnings scripts/watch.js",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"packages/{main,preload,renderer}/src/**/*.ts\" --write",
    "fix:lint": "eslint packages/{main,preload,renderer}/src --ext js,ts,tsx --fix",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "npm run build:preload:types && tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "run-p typecheck:*",
    "release:mac": "electron-builder -p always -m --x64",
    "dist:mac": "npm run build && electron-builder --config .electron-builder.config.js -m --x64",
    "download-ffmpeg-mac": "mkdirp ffmpeg/darwin-x64 && cd ffmpeg/darwin-x64 && wget https://github.com/mifi/ffmpeg-build-script/releases/download/4.4.1-2/ffmpeg -O ffmpeg && wget https://github.com/mifi/ffmpeg-build-script/releases/download/4.4.1-2/ffprobe -O ffprobe && chmod +x ffmpeg && chmod +x ffprobe",
    "download-ffmpeg-linux": "mkdirp ffmpeg/linux-x64 && cd ffmpeg/linux-x64 && wget https://github.com/mifi/ffmpeg-builds/releases/download/4.4.1/ffmpeg-release-amd64-static.tar.xz -O ffmpeg.xz && tar xvf ffmpeg.xz ffmpeg-4.4.1-amd64-static/ffmpeg ffmpeg-4.4.1-amd64-static/ffprobe --strip-components=1",
    "download-ffmpeg-windows": "mkdirp ffmpeg/win32-x64 && cd ffmpeg/win32-x64 && npx download-cli https://github.com/mifi/ffmpeg-builds/releases/download/4.4.1/ffmpeg-n4.4.1-2-gcc33e73618-win64-gpl-4.4.zip --out . --filename ffmpeg.zip && 7z x ffmpeg.zip && npx shx mv ffmpeg-n4.4.1-2-gcc33e73618-win64-gpl-4.4/bin/ffmpeg.exe ./ && npx shx mv ffmpeg-n4.4.1-2-gcc33e73618-win64-gpl-4.4/bin/ffprobe.exe ./",
    "vite:clean": "rimraf node_modules/.vite",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.9.2",
    "@mdx-js/rollup": "^2.1.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@swc/core": "^1.2.177",
    "@types/color-hash": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.4",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/sanitize-html": "^2.6.2",
    "@types/sqlite3": "^3.1.8",
    "@types/suncalc": "^1.8.1",
    "@types/tail": "^2.2.1",
    "@types/tcp-port-used": "^1.0.1",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-react": "^1.1.4",
    "cross-env": "^7.0.3",
    "dts-for-context-bridge": "^0.7.1",
    "electron": "^17.0.1",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "happy-dom": "^2.55.0",
    "nano-staged": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.18.0",
    "prettier": "^2.5.1",
    "prettier-airbnb-config": "^1.0.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-swc": "^0.2.1",
    "simple-git-hooks": "2.7.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-checker": "^0.4.6",
    "vitest": "^0.12.4"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mdx-js/react": "^2.1.1",
    "@mui/icons-material": "^5.6.0",
    "@mui/material": "^5.6.0",
    "@nibus/cli": "file:.yalc/@nibus/cli",
    "@nibus/core": "file:.yalc/@nibus/core",
    "@nibus/detection": "file:.yalc/@nibus/detection",
    "@nibus/mibs": "file:.yalc/@nibus/mibs",
    "@novastar/codec": "file:.yalc/@novastar/codec",
    "@novastar/native": "file:.yalc/@novastar/native",
    "@novastar/net": "file:.yalc/@novastar/net",
    "@novastar/screen": "file:.yalc/@novastar/screen",
    "@novastar/serial": "file:.yalc/@novastar/serial",
    "@reduxjs/toolkit": "^1.8.1",
    "@sarakusha/material-ui-chip-input": "^2.0.0",
    "ajv": "^8.11.0",
    "ansicolor": "^1.1.100",
    "bonjour-hap": "^3.6.3",
    "color-hash": "^2.0.1",
    "debug": "^4.3.4",
    "electron-log": "^4.4.6",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.5",
    "execa": "^6.1.0",
    "express": "^4.18.0",
    "file-type": "^17.1.1",
    "formidable": "^2.0.1",
    "helmet": "^5.0.2",
    "highcharts": "^10.0.0",
    "highcharts-react-official": "^3.1.0",
    "hotkeys-js": "^3.8.7",
    "iput": "^2.0.1",
    "lodash": "^4.17.21",
    "notistack": "^2.0.3",
    "p-map": "^5.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-imask": "^6.4.2",
    "react-redux": "^7.2.8",
    "sanitize-html": "^2.7.0",
    "serialport": "^10.4.0",
    "sqlite3": "^5.0.3",
    "suncalc": "^1.9.0",
    "supports-color": "^8.1.1",
    "tail": "^2.2.4",
    "tcp-port-used": "^1.0.2",
    "tiny-typed-emitter": "^2.1.0",
    "typed-struct": "file:.yalc/typed-struct",
    "typeface-roboto": "^1.1.13",
    "valid-url": "^1.0.9"
  },
  "overrides": {
    "@emotion/react": "^11.9.0",
    "prettier": "^2.5.1",
    "debug": "^4.3.4",
    "onetime": "^5",
    "supports-color": "^8.1.1"
  },
  "resolutions": {
    "prettier": "^2.5.1",
    "debug": "^4.3.4",
    "supports-color": "^8.1.1",
    "onetime": "^5"
  }
}
